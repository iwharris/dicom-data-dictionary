{
    "name": "@iwharris/dicom-data-dictionary",
    "version": "0.0.1",
    "description": "Dictionary of DICOM data elements, built directly from the published standard at dicom.nema.org",
    "main": "dist/index.js",
    "scripts": {
        "build": "ts-node build/index.ts",
        "compile": "tsc -p .",
        "fetch": "wget -N -P './tmp' 'http://dicom.nema.org/medical/dicom/current/source/docbook/part06/part06.xml'",
        "prettier": "prettier --check {src,build}/**/*.[jt]s",
        "test": "jest",
        "test:coverage": "jest --coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iwharris/dicom-data-dictionary.git"
    },
    "keywords": [
        "dicom",
        "dictionary"
    ],
    "author": "Ian Harris",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/iwharris/dicom-data-dictionary/issues"
    },
    "homepage": "https://github.com/iwharris/dicom-data-dictionary#readme",
    "eslintConfig": {
        "extends": "@iwharris/eslint-config"
    },
    "jest": {
        "transform": {
            "^.+\\.ts?$": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": "/test/.*\\.(test|spec)?\\.(ts)$",
        "moduleFileExtensions": [
            "ts",
            "js"
        ]
    },
    "devDependencies": {
        "@iwharris/eslint-config": "0.0.3",
        "@iwharris/prettier-config": "0.0.3",
        "@types/jest": "^24.0.23",
        "@types/jsonpath": "^0.2.0",
        "@types/node": "^12.12.14",
        "@types/request-promise-native": "^1.0.17",
        "@types/xml2js": "^0.4.5",
        "@typescript-eslint/eslint-plugin": "^2.10.0",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.1",
        "handlebars": "^4.5.3",
        "jest": "^24.9.0",
        "jsonpath": "^1.0.2",
        "prettier": "^1.19.1",
        "ts-jest": "^24.2.0",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.3",
        "xml2js": "^0.4.22"
    },
    "dependencies": {}
}
